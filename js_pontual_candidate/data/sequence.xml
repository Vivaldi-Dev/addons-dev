<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <data noupdate="1">
        <record id="seq_shift_swap" model="ir.sequence">
            <field name="name">Sequência de Troca de Turno</field>
            <field name="code">shift.swap</field>
            <field name="prefix">TSWAP/</field>
            <field name="padding">4</field>
            <field name="number_increment">1</field>
			<field name="number_next_actual">1</field>
			<field name="implementation">standard</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>


    <data noupdate="1">
        <record id="seq_certificate_request" model="ir.sequence">
            <field name="name">Sequência de Solicitação de Certificados</field>
            <field name="code">certificate.request</field>
            <field name="prefix">CERT/</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
			<field name="number_next_actual">1</field>
			<field name="implementation">standard</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>


</odoo>